.searchbar {
  background-color: #41be57;
  padding: 20rpx;
}

.navigator-search {
  width: 100%;
  background-color: #fff;
  height: 60rpx;
  border-radius: 10rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: center;
  background-size: 8%;
}

.search-input-group {
  width: 100%;
  background-color: #fff;
  height: 60rpx;
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.search-input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 30rpx;
}
